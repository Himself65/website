    "lib0": "^0.2.9",
    "lit-html": "^1.1.0",
    "prosemirror-example-setup": "^1.0.1",
    "prosemirror-state": "^1.2.3",
    "prosemirror-view": "^1.9.10",
    "quill": "^1.3.7",
    "quill-cursors": "^2.2.2",
    "y-indexeddb": "^9.0.2",
    "y-prosemirror": "^0.2.6",
    "y-quill": "0.0.2",
    "y-webcomponents": "0.0.1",
    "y-webrtc": "^10.1.0",
    "y-websocket": "^1.2.0",
